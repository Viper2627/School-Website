<form action="/download" method="POST">
  <div class="jumbotron">
    

    
  <h1>This is Registration List</h1>
  <button type="submit"> Excel</button>
 
</div>




  
</form>
<div class="jumbotron">

    <% registrations.forEach(function(registrations){
        %>
        <div class="container">

     
        <form>
            <div class="head" style="background-color: cadetblue;height: 30px;">New Student</div>
            <div class="form-group">
                <label for="Student">Student:</label>
              <input type="text" class="form-control"  value="<%= registrations.Student %>" readonly>
            
            </div>
            <div class="form-group">
              <label for="Guardian">Guardian:</label>
              <input type="text" class="form-control" value="<%= registrations.Guardian %>" readonly>
            </div>
            <div class="form-group">
                <label for="Contact">Contact Number:</label>
                <input type="text" class="form-control" value="<%= registrations.number %>" readonly>
              </div>
              <div class="form-group">
                <label for="Address">Address:</label>
                <input type="text" class="form-control" value="<%= registrations.Address %>" readonly>
              </div>
              <div class="form-group">
                <label for="City">City:</label>
                <input type="text" class="form-control" value="<%= registrations.City %>" readonly>
              </div>
              <div class="form-group">
                <label for="DOB">DOB:</label>
                <input type="text" class="form-control" value="<%= registrations.dob %>" readonly>

              </div>
              <div class="form-group">
                <label for="Gender">Gender:</label>
                <input type="text" class="form-control" value="<%= registrations.Gender %>" readonly>

              </div>
              
            
          </form>
        
        </div>
       
    <% }); %>
  

